class checkMeta(type):
    def __new__(cls, name, bases, attrs):
        for attr_name, attr_value in attrs.items():
            if hasattr(cls, '__annotations__'):
                if isinstance(attr_value, cls.__annotations__[attr_name]):
                    print(f"{attr_name} handisanum e jisht tipi")
            else:
                print('this class cannot be checked because it does not contain the "annotations" field')
        return super().__new__(cls, name, bases, attrs)

class Point(metaclass=checkMeta):
    name:str
    age:int
    name = 10
    age = ''

p = Point()
print(p.__annotations__)
p.name = 10
p.age = "ff"
